<IfModule mod_rewrite.c>
RewriteEngine On
RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/?$ /index.php?p=$1&var1=$2&var2=$3&var3=$4 [L]

RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/?$ /index.php?p=$1&var1=$2&var2=$3 [L]

RewriteRule ^([a-zA-Z0-9_-]+)/([a-zA-Z0-9_-]+)/?$ /index.php?p=$1&var1=$2 [L]

RewriteRule ^([a-zA-Z0-9_-]+)/?$ /index.php?p=$1 [L]

</IfModule>